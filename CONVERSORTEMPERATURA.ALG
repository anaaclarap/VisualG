Algoritmo "ConversorTemperatura"

Var
 celsius, fahrenheit, kelvin : real

Inicio
 Escreval("Digite o valor de celsius:")
 Leia(celsius)
 fahrenheit <- (celsius * 9/5) + 32
 Escreval("O valor em fahrenheit é:", fahrenheit)
 kelvin <- celsius + 273,15
 Escreval ("O valor em kelvin é:", kelvin)


Fimalgoritmo